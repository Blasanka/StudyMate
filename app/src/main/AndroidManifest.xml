<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.mad.studymate">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/studymate_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/studymate_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.DarkActionBar">
        <activity android:name=".activity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".activity.AddQuizActivity" />
        <activity android:name=".activity.AnswerQuizActivity" />
        <activity android:name=".activity.ViewNoteActivity" />
        <activity android:name=".activity.AddNoteActivity" />
        <activity android:name=".activity.AddTaskActivity"></activity>
    </application>

</manifest>